<Page xmlns="http://schemas.nativescript.org/tns.xsd" navigatingTo="onNavigatingTo">
  <StackLayout>
    <Label text="Tap the button" class="title"/>
    <Button text="TAP" tap="{{ onTap }}" />
    <Label text="{{ username }}" class="message" textWrap="true"/>
    <Label text="{{ email }}" class="message" textWrap="true"/>
    <Label text="{{ authcode }}" class="message" textWrap="true"/>
  </StackLayout>
</Page>